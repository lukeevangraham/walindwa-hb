<div class="timeline-image">
    <img src="https://admin.moreleft.com{{image.url}}" alt="{{image.alternativeText}}" class="rounded-circle img-fluid">
</div>
<div class="timeline-panel">
    <div class="timeline-heading">
        <h4>{{time}}</h4>
        <hv class="subheading">{{heading}}</hv>
    </div>
    <div class="timeline-body">
        <p class="text-muted">{{body}}</p>
    </div>
</div>