{{> main/breadcrumb currentTitle=this.title}}

<!-- - - - - - - - - - - - - - Page Content - - - - - - - - - - - - - - - - -->

<div class="gt-page-content-wrap gt-no-sidebar">

    <div class="container">

        <div class="row">

                <div id="newsletterTop" class="col-xs-12" style="z-index: 200;">

                    <section class="gt-content-element">

                        <h2>Newsletters</h2>
                        <h4>Please view our latest newsletter</h4>

                        {{#each newsletters}} {{#if @first}} <ul class="gt-list gt-type-1">
                            <li> {{#if link}} <a class="gt-paypal-link" href="{{link}}"> {{/if}} {{#unless link}} <a
                                        href="https://admin.walindwa.org{{file.url}}" target="_blank">{{/unless}} {{month}} {{year}}</a>
                                    -
                                    {{name}}</li>
                        </ul> {{/if}} {{/each}}

                    </section>

                </div>

            <div class="col-md-6 col-md-push-6 gt-text-content-element">
                <h4>Subscribe</h4>
                <a href="https://walindwa.us7.list-manage.com/subscribe?u=11622771fa865ffca225b78de&id=24bb4c515f"
                    class="gt-btn navA">Click here to subscribe to our newsletter</a>
            </div>


            <div class="col-md-6 col-md-pull-6 gt-text-content-element">
                <h4>Archive</h4>
                <ul class="gt-list gt-type-1">
                    {{#each newsletters}}
                    {{#unless @first}}
                    {{#if link}}
                    <li><a class="gt-paypal-link" href="{{link}}">
                            {{/if}}
                            {{#if file}}
                    <li><a class="gt-paypal-link" href="https://admin.walindwa.org{{file.url}}" target="_blank"
                            rel="noopener noreferrer">
                            {{/if}}
                            {{month}} {{year}}</a> - {{name}}</li>
                    {{/unless}}
                    {{/each}}
                </ul>
            </div>






            <!--/ .col -->

        </div>
        <!--/ .row -->

    </div>
    <!--/ .container -->

</div>

<!-- - - - - - - - - - - - - - End of Page Content - - - - - - - - - - - - - - - - -->